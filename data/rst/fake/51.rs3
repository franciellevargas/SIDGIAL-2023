    <segment id="1" parent="35" relname="span">Consultoria internacional elege Temer</segment>
    <segment id="34" parent="1" relname="comparison"> como o presidente mais impopular do planeta. </segment>
    <segment id="3" parent="36" relname="evaluation"> Temer é o cara!</segment>
    <segment id="4" parent="36" relname="span"> Ele conseguiu uma façanha mundial</segment>
    <segment id="2" parent="4" relname="nonvolitional-cause"> ao superar Dilma Rousseff no quesito impopularidade. </segment>
    <segment id="5" parent="39" relname="span"> O peemedebista foi considerado o presidente com a maior taxa de rejeição do planeta,</segment>
    <segment id="6" parent="38" relname="span"> perdendo até para o presidente da Venezuela,</segment>
    <segment id="7" parent="6" relname="elaboration"> Nicolás Maduro.</segment>
    <segment id="8" parent="44" relname="span"> A pesquisa, que foi executada pelo Grupo Eurasia, </segment>
    <segment id="15" parent="8" relname="elaboration">coletou dados durante os meses de agosto e setembro deste ano.</segment>
    <segment id="12" parent="14" relname="evaluation"> Michel, o homem que prometeu estancar a sangria,</segment>
    <segment id="14" parent="45" relname="span"> consegiu míseros 3% de aprovação popular, a menor taxa do mundo, seguido por:</segment>
    <segment id="18" parent="48" relname="span"> Jacob Zuma,África do Sul,</segment>
    <segment id="16" parent="18" relname="parenthetical"> (corrupto)</segment>
    <segment id="17" parent="49" relname="same-unit"> 18% de apoio popular.</segment>
